    "require": {
        "php": "^7.0",
        "slim/slim": "^3.1",
        "slim/twig-view": "2.1.1",
        "illuminate/database": "5.4.36",
        "slim/flash": "^0.4.0",
        "leafo/scssphp": "^0.7.7",
        "matthiasmullie/minify": "^1.3",
        "monolog/monolog": "^1.24",
        "twig/extensions": "^1.5",
        "symfony/translation": "^4.2",
        "symfony/twig-bridge": "^4.2",
        "robmorgan/phinx": "^0.10.7",
        "symfony/console": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "Controllers\\": "src/controllers/",
            "Models\\": "src/models/",
            "Migration\\": "db/migrator"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "compile-assets": "php ./assets/compile.php"
    }

}
